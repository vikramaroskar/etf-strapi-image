{
  "kind": "collectionType",
  "collectionName": "funds",
  "info": {
    "singularName": "fund",
    "pluralName": "funds",
    "displayName": "Fund",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "fundManager": {
      "type": "string"
    },
    "type": {
