      "enum": [
        "open",
        "closed"
      ]
    },
    "allotmentDate": {
      "type": "date"
    },
    "benchmarkIndex": {
      "type": "string"
    },
    "minimumInvestmentAmount": {
      "type": "decimal"
    },
    "expenseRatio": {
      "type": "decimal"
    },
    "netAum": {
      "type": "decimal"
    },
    "creationUnitSize": {
      "type": "integer"
    },
    "symbols": {
      "type": "richtext"
    },
    "allotmentPrice": {
      "type": "decimal"
    },
    "authorized_participants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::authorized-participant.authorized-participant",
      "inversedBy": "funds"
    },
    "fund_analyses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::fund-analysis.fund-analysis",
      "mappedBy": "fund"
    },
    "listings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::listing.listing",
      "mappedBy": "funds"
